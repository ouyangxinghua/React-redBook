.App
  width 100vw
  height 100vh
  .tabbar
    position fixed
    bottom 0
    width 100%
    height 40px
    font-size 16px
    display flex
    justify-content space-evenly
    align-items center
    z-index 99
    background-color #fff
    .open-file
      width 20%
      border 0
      background-color transparent
      outline none
      width 50px
      height 33px
    .nav-link
      .selected
        color #000000
        font-size 18px
  .redBook-main
    width 100%
    height 100%